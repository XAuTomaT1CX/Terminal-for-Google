{
	"name": "Google Terminal",
	"version": "0.0.4.7",
	"description": "Quick access to Google!",
	
	"icons": {
		"16": "icons/google-16.png",
		"128": "icons/google.png"
	},
	
	"permissions": [
		"contextMenus",
		"tabs",
		"http://*/",
		"https://*/"
	],
	
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"css": [],
			"js": ["js/content.js"]
		}
	],
	
	"background_page": "back.html",
	"options_page": "opt.html",
	
	"browser_action": {
		"default_title": "Google Terminal",
		"default_icon": "icons/google.png",
		"popup": "pop.html"
	}
}
